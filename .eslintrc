{
  "extends": "airbnb",
  "parser": "typescript-eslint-parser",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Babel": true,
    "React": true
  },
  "plugins": ["react", "typescript"],
  "rules": {
    "indent": ["error", 2],
    "react/jsx-indent": ["error", 2],
    "react/jsx-indent-props": ["error", 2],
    "comma-dangle": ["error", "never"]
  }
}
